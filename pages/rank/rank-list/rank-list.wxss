page{
  background: #EFF2F5;
}
.rank{
  display: flex;
  font-size: 30rpx;
  height: 80rpx;
  margin: 20rpx auto;
  justify-content: space-between;
  align-items: center;
  font-weight: bold;
  color: #ffffff;
  background: #4871E2;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
    
}
.rank_copy{
  display: flex;
  font-size: 24rpx;
  height: 60rpx;
  margin: 20rpx auto;
  justify-content: space-between;
  align-items: center;
  font-weight: bold;
  color: #ffffff;
  background: #4871E2;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
}
.font-color{
  color:#f4ea2a;
  font-size:36rpx;
  font-weight: bold;
  padding:0rpx 10rpx;
}
.font-color_copy{
  color:#f4ea2a;
  font-size:30rpx;
  font-weight: bold;
  padding:0rpx 10rpx;
}
.small_box{
  background: #EFF2F5;
  max-height: 700rpx;
  overflow: hidden;
  position: relative;
}
.rank_title_copy{
  height:90rpx;
  background: #ffffff;
  font-weight: bold;
  margin-bottom: 4rpx;
  display: flex;
  min-height:80rpx;
  color:#333333;
  font-size:24rpx;
  padding:0rpx 30rpx;
  justify-content: space-between;
  align-items: center;
}
.rank_title,.rank_content{
  display: flex;
  min-height:80rpx;
  color:#333333;
  font-size:32rpx;
  padding:0rpx 30rpx;
  justify-content: space-between;
  align-items: center;
}
.rank_title{
  height:120rpx;
  background: #ffffff;
  font-weight: bold;
  margin-bottom: 4rpx
}
.rank_content{
  width:100%;
  margin-bottom: 20rpx;
  background: #ffffff;
  color: #666666;
  box-sizing: border-box
}
.rank_content_copy{
  display: flex;
  min-height:60rpx;
  color:#333333;
  font-size:24rpx;
  padding:0rpx 30rpx;
  justify-content: space-between;
  align-items: center;
  width:100%;
  margin-bottom: 10rpx;
  background: #ffffff;
  color: #666666;
  box-sizing: border-box
}
.isSelf{
  width:100%;
  margin-bottom: 20rpx;
  background:#4871E2;
  color:#ffffff;
  box-sizing: border-box;
  font-weight: bold;
}
.rank_box{
  margin-top:40rpx;
}
.rank-rank{
  width:15%;
  text-align: center;
  position: relative;
}
.rank-name{
  width:25%;
  text-align: center;
  word-break:break-all;

}
.rank-special{
  width:40%;
  text-align: center;

}
.rank-score{
  width:20%;
  text-align: center;
}
.rank_content image{
  width:64rpx;
  height:64rpx;
}
.rank_content_copy image{
  width:45rpx;
  height:45rpx;
}
.rank-font{
  font-size:32rpx;
  font-weight: bold;
  color:#ffffff;
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%,-50%);

}
.bottom_box_copy{
  height:50rpx;
  text-align: center;
  color:#666666;
  font-size:28rpx;
}
.button_box{
  height:120rpx;
  width:750rpx;
  background: #FFFFFF;
  box-shadow: 0px 0px 32px 5px rgba(186, 201, 222, 0.45);
  position: fixed;
  bottom:0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0rpx 36rpx;
  box-sizing: border-box;

}
.img_box image{
  height:65rpx;
  width:65rpx;
}
.img_box{
  font-size:24rpx;
  text-align: center;
}
.bottom_box{
  height:240rpx;
  width:750rpx;
  position: fixed;
  bottom: 0rpx;
  background: #FFFFFF;
  z-index:100;
}
.bottom_item{
  height: 80rpx;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  line-height: 80rpx;
  border-bottom:1px solid #eeeeee;
  color:#333333;
}
button{
  padding:0rpx !important;
  background: #FFFFFF !important;
  width:100% !important;
  font-weight:normal !important;
}
.bottom_item:nth-of-type(2){
  border-bottom:none !important;

}
.poster-wrap {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  /* padding: 0 48rpx; */
  box-sizing: border-box;
  background-color: #fff;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
}
.poster-wrap-bg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.6);
}
.poster-wrap-close {
  width: 64rpx;
  height: 80rpx;
  position: absolute;
  right: 32rpx;
  top: 0;
  transform: translateY(-100%);
}
.bg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 99;
}


.modal {
  position: absolute;
  top: 400rpx;
  left: 105rpx;
  width: 540rpx;
  background-color: #ffffff;
  border-radius: 16rpx;
}

.modal .title {
  margin: 32rpx 0;
  width: 100%;
  text-align: center;
  color: #333333;
  font-size: 34rpx;
}

.modal .phone-num {
  width: 100%;
  margin-bottom: 60rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  color: #666666;
  padding: 0 46rpx;
  text-align: left;
}

.btn-area {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  border-top: 1rpx solid #cccccc;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
}

.btn-area view {
  border-bottom-left-radius: 16rpx;
}

.button-hover {
  background-color: #ffffff;
}

.btn-area view,
.btn-area button {
  width: 50% !important;
  height: 96rpx;
  line-height: 96rpx;
  color: #333333;
  font-size: 36rpx;
  text-align: center;
  background-color: #ffffff;
  border: none;
  border-bottom-right-radius: 16rpx;
}
/* 海报样式 */
.poster-wrap {
  padding: 30rpx 0;
}

.poster-wrap-title {
  border-bottom: 1rpx solid #e5e5e5;
}

.poster-wrap-title>view:first-child {
  color: #333333;
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}

.poster-wrap-title>view:last-child {
  margin-bottom: 30rpx;
  color: #999999;
  font-size: 24rpx;
  text-align: center;
}

.poster {
  margin: 48rpx 188rpx;
  width: 374rpx;
  height: 667rpx;
}

.poster canvas {
  margin-left: 0;
  width: 100%;
  height: 100%;
  box-shadow: 0 2rpx 16rpx 0rpx rgba(0, 0, 0, 0.2);
}

.poster-footer {
  margin-left: 48rpx;
  width: 654rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #4871E2;
  color: #ffffff;
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}
.qrcode_box{
  box-shadow: 0px 0px 16px 5px rgba(186, 201, 222, 0.45);
  height: 125rpx;
  width: 750rpx;
  background: #ffffff;
  position: absolute;
  bottom: 10rpx;
  border: 1px solid #eeeeee;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
}
.qrcode_box image{
  height:120rpx;
  width:120rpx;

}
.qrcode_box .big_font{
  font-size:30rpx;
  color:#333333;
  font-weight: bold;
  text-align: center;
  margin-bottom:10rpx;
}
.small_font{
  font-size:22rpx;
  color:#666666;
  text-align: center;
}