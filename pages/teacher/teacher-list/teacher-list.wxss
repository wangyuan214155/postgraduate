.teacher_box{
  width: 690rpx;
  margin: 20rpx 30rpx;
  font-size: 32rpx;
  color: #666666;
  background: #FFFFFF;
  box-shadow: 0px 0px 32px 0px rgba(186, 201, 222, 0.45);
  border-radius: 16px;
  padding: 20rpx 0rpx;
