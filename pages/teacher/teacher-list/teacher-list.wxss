}
.teacher_item{
  display: flex;
  align-items: center;
  height:80rpx;
  padding:0rpx 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.warn_font{
  color:#4871E2;
  font-size:36rpx;
  font-weight: bold;
}
.studentList{
