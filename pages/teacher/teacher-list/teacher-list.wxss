/* 海报样式 */
.poster-wrap {
  padding: 30rpx 0;
}

.poster-wrap-title {
  border-bottom: 1rpx solid #e5e5e5;
}

.poster-wrap-title>view:first-child {
  color: #333333;
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}

.poster-wrap-title>view:last-child {
  margin-bottom: 30rpx;
  color: #999999;
  font-size: 24rpx;
  text-align: center;
}

.poster {
  margin: 48rpx 188rpx;
  width: 374rpx;
  height: 667rpx;
}

.poster canvas {
  margin-left: 0;
  width: 100%;
  height: 100%;
  box-shadow: 0 2rpx 16rpx 0rpx rgba(0, 0, 0, 0.2);
}

.poster-footer {
  margin-left: 48rpx;
  width: 654rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #4871E2;
  color: #ffffff;
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}
.poster-wrap {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  /* padding: 0 48rpx; */
  box-sizing: border-box;
  background-color: #fff;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
}
.poster-wrap-bg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.6);
}
.poster-wrap-close {
  width: 64rpx;
  height: 80rpx;
  position: absolute;
  right: 32rpx;
  top: 0;
  transform: translateY(-100%);
}
.bg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 99;
}


.modal {
  position: absolute;
  top: 400rpx;
  left: 105rpx;
  width: 540rpx;
  background-color: #ffffff;
  border-radius: 16rpx;
}

.modal .title {
  margin: 32rpx 0;
  width: 100%;
  text-align: center;
  color: #333333;
  font-size: 34rpx;
}

.modal .phone-num {
  width: 100%;
  margin-bottom: 60rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  color: #666666;
  padding: 0 46rpx;
  text-align: left;
}

.btn-area {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  border-top: 1rpx solid #cccccc;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
}

.btn-area view {
  border-bottom-left-radius: 16rpx;
}

.button-hover {
  background-color: #ffffff;
}

.btn-area view,
.btn-area button {
  width: 50% !important;
  height: 96rpx;
  line-height: 96rpx;
  color: #333333;
  font-size: 36rpx;
  text-align: center;
  background-color: #ffffff;
  border: none;
  border-bottom-right-radius: 16rpx;
}
